config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Joystick",
"language": "GDScript",
"path": "res://joystick/Joystick.gd"
} ]
_global_script_class_icons={
"Joystick": ""
}

[application]

config/name="amogus rage game"
config/description="cool amogus game made by flooferland lol"
run/main_scene="res://levels/Splash Screen.tscn"
boot_splash/image="res://sprites/other/black.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
config/auto_accept_quit=false

[autoload]

Global="*res://scripts/singletons/Global.gd"
GameInfo="*res://scripts/singletons/GameInfo.gd"
SaveFile="*res://scripts/singletons/SaveFile.gd"
QuickMafs="*res://scripts/singletons/QuickMafs.gd"
Logger="*res://scripts/singletons/Logger.gd"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/narrowing_conversion=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1366
window/size/test_height=768
window/vsync/use_vsync=false
window/stretch/aspect="expand"
"window/size/Minimum Width"=1100
"window/size/Minimum Height"=800

